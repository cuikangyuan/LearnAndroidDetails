<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:height="200dp"
    android:width="200dp"
    android:viewportHeight="100"
    android:viewportWidth="100">
    <group
        android:name="sun"
        android:pivotX="60"
        android:pivotY="50"
        android:rotation="0">

        <path
            android:name="path_sun"
            android:strokeColor="@color/red"
            android:strokeWidth="2"
            android:pathData="
            M 50, 50
            a 10, 10, 0, 1, 0, 20, 0
            a 10, 10, 0, 1, 0, -20, 0
            M 50, 50
            L 70, 50
            "
            />

        <group
            android:name="earth"
            android:pivotX="60"
            android:pivotY="50"
            android:rotation="0">
            <path
                android:name="path_earth"
                android:fillColor="@color/green"
                android:pathData="
                    M 70, 50
                    a 5, 5, 0, 1, 0, 10, 0
                    a 5, 5, 0, 1, 0, -10, 0"
                />

            <group
                android:name="moon">
                <path
                    android:name="path_moon"
                    android:fillColor="@color/blue"
                    android:pathData="
                    M 90, 50
                    a 4, 4, 0, 1, 0, 8, 0
                    a 4, 4, 0, 1, 0, -8, 0"
                    />
            </group>

        </group>

    </group>

</vector>